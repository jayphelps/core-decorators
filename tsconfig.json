{
  "include": ["src/**/*.ts"],
  "compilerOptions": {
    "outDir": "lib",
    "target": "es5",
    "module":"commonjs",
    "declaration": true,
    "allowUnreachableCode": true,
    "lib": [
      "es5",
      "dom",
      "es2015","es2015.core", "es2015.collection"
    ],
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true
  }
}
